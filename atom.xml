<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Debian, Ubuntu and macOS Howto Guides</title>
 <link href="https://justinhartman.github.io/nix/atom.xml" rel="self"/>
 <link href="https://justinhartman.github.io/nix/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://justinhartman.github.io/nix/</id>
 <author>
   <name>Justin Hartman</name>
   <email>justin@hartman.me</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://justinhartman.github.io/nix{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://justinhartman.github.io/nix{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
